{
  "name": "alecgerona.io",
  "description": "Alec Gerona's personal website.",
  "version": "0.1.0",
  "author": "Alexandre Gerona <alecgerona@gmail.com>",
  "scripts": {
    "dev": "yarn type-check && gatsby develop",
    "build": "yarn type-check && gatsby build",
    "lint:js": "eslint **/*.tsx --fix",
    "lint:style": "stylelint **/*.css --ignore-path .gitignore --fix --allow-empty-input",
    "lint": "yarn lint:js && yarn lint:style",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "type-check": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "*.tsx": "yarn lint:js",
    "*.css": "yarn lint:style"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit",
      "commit-msg": "yarn commit-msg"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@tailwindcss/typography": "0.2.0",
    "gatsby": "2.24.58",
    "gatsby-image": "2.4.17",
    "gatsby-plugin-feed": "2.5.11",
    "gatsby-plugin-google-analytics": "2.3.13",
    "gatsby-plugin-manifest": "2.4.28",
    "gatsby-plugin-offline": "3.2.27",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-robots-txt": "1.5.1",
    "gatsby-plugin-sharp": "2.6.35",
    "gatsby-plugin-sitemap": "2.4.12",
    "gatsby-plugin-typescript": "2.4.19",
    "gatsby-remark-autolink-headers": "2.3.12",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.3.29",
    "gatsby-remark-prismjs": "3.5.12",
    "gatsby-remark-reading-time": "1.1.0",
    "gatsby-remark-smartypants": "2.3.10",
    "gatsby-source-filesystem": "2.3.29",
    "gatsby-transformer-json": "2.4.11",
    "gatsby-transformer-remark": "2.8.34",
    "gatsby-transformer-sharp": "2.5.14",
    "prismjs": "1.21.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-spring": "8.0.27",
    "typeface-dm-sans": "1.1.3",
    "typeface-lora": "0.0.72"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.7",
    "@types/node": "14.6.4",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/vfile-message": "2.0.0",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "commitizen": "4.2.1",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "gatsby-plugin-postcss": "2.3.11",
    "husky": "4.2.5",
    "lint-staged": "10.2.13",
    "prettier": "2.1.1",
    "semantic-release": "17.1.1",
    "stylelint": "13.7.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "tailwindcss": "1.8.4",
    "typescript": "4.0.2"
  },
  "keywords": [
    "gatsby",
    "tailwindcss",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alecgerona/alecgerona.io"
  },
  "bugs": {
    "url": "https://github.com/alecgerona/alecgerona.io/issues"
  }
}
