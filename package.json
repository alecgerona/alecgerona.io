{
  "name": "alecgerona.io",
  "description": "Alec Gerona's personal website.",
  "version": "1.0.0",
  "author": "Alexandre Gerona <alecgerona@gmail.com>",
  "scripts": {
    "dev": "yarn type-check && gatsby develop",
    "build": "yarn type-check && gatsby build",
    "lint:js": "eslint **/*.tsx --fix",
    "lint:style": "stylelint **/*.css --ignore-path .gitignore --fix --allow-empty-input",
    "lint": "yarn lint:js && yarn lint:style",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "type-check": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.tsx": "yarn lint:js",
    "*.css": "yarn lint:style"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit",
      "commit-msg": "yarn commit-msg"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@tailwindcss/typography": "0.4.1",
    "gatsby": "3.12.0",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-feed": "3.12.0",
    "gatsby-plugin-google-analytics": "3.12.0",
    "gatsby-plugin-manifest": "3.12.0",
    "gatsby-plugin-offline": "4.12.0",
    "gatsby-plugin-react-helmet": "4.12.0",
    "gatsby-plugin-robots-txt": "1.6.9",
    "gatsby-plugin-sharp": "3.12.0",
    "gatsby-plugin-sitemap": "3.3.0",
    "gatsby-plugin-typescript": "3.12.0",
    "gatsby-remark-autolink-headers": "3.2.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "4.2.0",
    "gatsby-remark-prismjs": "4.2.0",
    "gatsby-remark-reading-time": "1.1.0",
    "gatsby-remark-smartypants": "3.2.0",
    "gatsby-source-filesystem": "3.12.0",
    "gatsby-transformer-json": "3.12.0",
    "gatsby-transformer-remark": "3.2.0",
    "gatsby-transformer-sharp": "3.12.0",
    "postcss": "8.3.6",
    "prismjs": "1.24.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-spring": "9.2.4",
    "typeface-dm-sans": "1.1.13",
    "typeface-lora": "1.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@types/node": "14.17.11",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/react-helmet": "6.1.2",
    "@types/vfile-message": "2.0.0",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "commitizen": "4.2.4",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.24.0",
    "gatsby-plugin-postcss": "4.12.0",
    "husky": "5.2.0",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "semantic-release": "17.4.6",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "21.0.0",
    "tailwindcss": "2.2.7",
    "typescript": "4.3.5"
  },
  "keywords": [
    "gatsby",
    "tailwindcss",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alecgerona/alecgerona.io"
  },
  "bugs": {
    "url": "https://github.com/alecgerona/alecgerona.io/issues"
  }
}
